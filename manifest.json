{
    "name": "HappyEdit",
    "description": "Vim-like editor for the Web",
    "manifest_version": 2,
    "version": "1",
    "app": {
        "background": {
            "scripts": ["background.js"]
        }
    },
    "permissions": [
        "fullscreen",
        "storage",
        "app.window",
        "clipboardRead",
        "clipboardWrite"
    ],
    "icons": {
        "16": "images/logo_16x16.png",
        "128": "images/logo_128x128.png"
    }
}
